{
  "name": "secondhand-spooks-backend",
  "version": "0.1.0",
  "description": "Backend API for Secondhand Spooks Inventory Management",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "node dist/utils/migrate.js",
    "import-csv": "ts-node src/utils/importCsv.ts",
